[
    {
        "components": [
            {
                "ename": "system_plugin\/text",
                "data_config": {
                    "richtext": {
                        "default": "\u9ed8\u8ba4\u6587\u672c",
                        "required": true,
                        "type": "string"
                    }
                },
                "html": "<div class=\"mod mod-text {{class}}\" style=\"margin:{{margin-top}} {{margin-right}} {{margin-bottom}} {{margin-left}};background:{{background}}\">{{richtext}}<\/div>"
            },
            {
                "ename": "system_plugin\/layout2coloums",
                "data_config": {
                    "left_width":{
                        "required":true,
                        "default":"50%"
                    },
                    "right_width":{
                        "required":true,
                        "default":"50%"
                    }
                },
                "html": "<div class=\"mod mod-layout2coloums mod-column mod-2column\" style=\"margin:{{margin-top}} {{margin-right}} {{margin-bottom}} {{margin-left}};\"><div class=\"devide-con\"><div class=\"cell\" style=\"width:{{left_width}};\" data-container=\"left_col\"><\/div><div class=\"cell\" style=\"width:{{right_width}};\" data-container=\"right_col\"><\/div> <\/div><\/div>"
            },
            {
                "ename": "system_plugin\/layout_floor",
                "data_config": {
                    "background-image": {
                        "require": false,
                        "type": "string"
                    },
                    "background-size": {
                        "require": false,
                        "type": "string"
                    },
                    "background-repeat": {
                        "require": false,
                        "type": "string"
                    },
                    "margin-right": {
                        "required": false,
                        "type": "string"
                    },
                    "margin-bottom": {
                        "required": false,
                        "type": "string"
                    },
                    "margin-top": {
                        "required": false,
                        "type": "string"
                    },
                    "margin-left": {
                        "required": false,
                        "type": "string"
                    },
                    "class": {
                        "default": "",
                        "require": false,
                        "type": "string"
                    },
                    "background-color": {
                        "default": "#f2f2f2",
                        "type": "string"
                    }
                },
                "html": "<div class=\"mod mod-layout_floor {{class}}\" data-role=\"sub-container\" data-container=\"sub-container\" style=\"background-size:{{background-size}};margin:{{margin-top}} {{margin-right}} {{margin-bottom}} {{margin-left}};background-color:{{background-color}};{{#background-image}}background-image:url({{background-image}});{{\/background-image}}{{#background-repeat}}background-repeat:{{background-repeat}}{{\/background-repeat}}\"><\/div>"
            },
            {
                "ename": "system_plugin\/head-title",
                "data_config": {
                    "title": {
                        "type": "string"
                    }
                },
                "html": "<title>{{title}}</title>"
            },
            {
                "ename": "system_plugin\/link-style",
                "data_config": {
                    "href": {
                        "type": "string"
                    }
                },
                "html": "<link rel=\"stylesheet\" type=\"text/css\" href=\"{{{href}}}\">"
            },
            {
                "ename": "system_plugin\/link-script",
                "data_config": {
                    "src": {
                        "type": "string"
                    }
                },
                "html": "<script src=\"{{{src}}}\"></script>"
            }
        ]
    }
]
